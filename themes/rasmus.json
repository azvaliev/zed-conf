{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Rasmus",
  "author": "azatvaliev",
  "themes": [
    {
      "name": "Rasmus Dark",
      "appearance": "dark",
      "style": {
        "background": "#1a1a19",
        "foreground": "#d1d1d1",
        "text": "#d1d1d1",
        "text.muted": "#6a6a69",
        "text.placeholder": "#767675",
        "text.disabled": "#4c4c4b",
        "text.accent": "#8db4d4",

        "border": "#323231",
        "border.variant": "#3a3a39",
        "border.focused": "#8db4d4",
