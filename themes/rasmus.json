        "editor.active_line.background": "#222221",
        "editor.highlighted_line.background": "#222221",
        "editor.line_number": "#6a6a69",
        "editor.active_line_number": "#b6b6b5",
        "editor.invisible": "#6a6a69",
        "editor.wrap_guide": "#323231",
        "editor.active_wrap_guide": "#3a3a39",

        "editor.document_highlight.bracket_background": "#272726",

        "terminal.foreground": "#d1d1d1",
        "terminal.background": "#1a1a19",
        "terminal.bright_foreground": "#eaeaea",
        "terminal.dim_foreground": "#1a1a19",

        "terminal.ansi.black": "#333332",
        "terminal.ansi.red": "#ff968c",
        "terminal.ansi.green": "#61957f",
        "terminal.ansi.yellow": "#ffc591",
        "terminal.ansi.blue": "#8db4d4",
        "terminal.ansi.magenta": "#de9bc8",
        "terminal.ansi.cyan": "#7bb099",
        "terminal.ansi.white": "#d1d1d1",

        "terminal.ansi.bright_black": "#4c4c4b",
